void setup() {
  size(500,500);
  background(#ad42f4); //purple
}

void draw() {
 if (mousePressed) {
  if( mouseX < width/3) {
      fill(#42b3f4); //light blue
      rect(mouseX, mouseY, 10,10);
  } else if(mouseX < 2/3.0*width) {
      fill(#42f468); //green blue
      rect(mouseX, mouseY, 10,10);
  } else {
      fill(#7af441); //yellow green
      rect(mouseX,mouseY,10,10);
    }
  }
}
